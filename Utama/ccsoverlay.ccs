            /* Overlay Loading */
        #loading-screen {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(255, 255, 255, 1); /* Warna putih dengan opacity penuh */
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            z-index: 9999;
            transition: opacity 1s ease-out;
        }

        /* Gambar Loading dengan Efek */
        .loading-image {
            width: 150px;
            animation: fadeInZoom 1.5s ease-in-out infinite alternate;
        }

        @keyframes fadeInZoom {
            0% {
                opacity: 0.5;
                transform: scale(0.9);
            }
            100% {
                opacity: 1;
                transform: scale(1.1);
            }
        }

