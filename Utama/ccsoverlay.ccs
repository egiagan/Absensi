.preloadChat span {
 width:15px;
 height:15px;
 background-color:var(--primary);
 border-radius:50%;
 display:inline-block;
 animation:1.4s ease-in-out infinite both bounce
}
.preloadChat span:first-child {
 animation-delay:-.32s
}
.preloadChat span:nth-child(2) {
 animation-delay:-.16s
}
.preloadChat span:nth-child(3) {
 animation-delay:0
}
@keyframes bounce {
 0%,100%,80% {
 transform:scale(0);
 opacity:.3
}
40% {
 transform:scale(1);
 opacity:1
}

}
@keyframes fadeInDown {
 from {
 opacity:0;
 transform:translate(-50%,-60%)
}
to {
 opacity:1;
 transform:translate(-50%,-50%)
}

